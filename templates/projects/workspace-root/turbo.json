{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "test": {
      "dependsOn": [
        "^build"
      ]
    },
    "lint:code": {
      "dependsOn": [
        "^build"
      ]
    },
    "lint:type": {
      "dependsOn": [
        "^build"
      ]
    }
  }
}
